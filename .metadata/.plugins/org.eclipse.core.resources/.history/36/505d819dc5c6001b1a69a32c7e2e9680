package operation;

public class Entites {

	private int number = 0;
	private String holder = "";
	private double balance = 0;

	public Entites(int number, String holder) {
		this.number = number;
		this.holder = holder;
	}

	public Entites(int number, String holder, double balance) {
		this.number = number;
		this.holder = holder;
		this.balance = balance;
	}

	public String getHolder() {
		return holder;
	}

	public void setHolder(String holder) {
		this.holder = holder;
	}

	public int getNumber() {
		return number;
	}

	public double getBalance() {
		return balance;
	}

}
